<h1>{{ title }}</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">id</th>
      <th scope="col">名前</th>
      <th scope="col">フルネーム</th>
      <th scope="col">国</th>
      <th scope="col">詳細</th>
    </tr>
  </thead>
  <tbody>
    {{#each composers}}
      <tr>
        <th scope="row">{{ id }}</th>
        <td>{{ lastName }}</td>
        <td>{{ fullName }}</td>
        <td>{{ countries }}</td>
        <td>{{ description }}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
